			EnableIn : BOOL (Description := "Enable Input - System Defined Parameter",
			                    Usage := Input,
			                    RADIX := Decimal,
			                    Required := No,
			                    Visible := No,
			                    ExternalAccess := Read Only);
			EnableOut : BOOL (Description := "Enable Output - System Defined Parameter",
			                     Usage := Output,
			                     RADIX := Decimal,
			                     Required := No,
			                     Visible := No,
			                     ExternalAccess := Read Only);
			cyl_dia : REAL (Usage := Input,
			                   RADIX := Float,
			                   Required := Yes,
			                   Visible := Yes,
			                   DefaultData := 0.00000000e+000);
			cyl_len : REAL (Usage := Input,
			                   RADIX := Float,
			                   Required := Yes,
			                   Visible := Yes,
			                   DefaultData := 0.00000000e+000);
			dist : REAL (Usage := Input,
			                RADIX := Float,
			                Required := Yes,
			                Visible := Yes,
			                DefaultData := 0.00000000e+000);
			gal : REAL (Usage := InOut,
			               RADIX := Float,
			               Required := Yes,
			               Visible := Yes);
		END_PARAMETERS

		LOCAL_TAGS
			al : REAL (RADIX := Float,
			             ExternalAccess := None,
			             DefaultData := 0.00000000e+000);
			vol : REAL (RADIX := Float,
			              ExternalAccess := None,
			              DefaultData := 0.00000000e+000);
			cyl_rad : REAL (RADIX := Float,
			                  ExternalAccess := None,
			                  DefaultData := 0.00000000e+000);
		END_LOCAL_TAGS

		ST_ROUTINE Logic 
			'cyl_rad:=cyl_dia/2;
			'al:=3.1415*cyl_rad**2-cyl_rad**2*ACOS((cyl_rad-dist)/cyl_rad)+(cyl_rad-dist)*SQRT(2*cyl_rad*dist-dist**2);
			'vol:=al*cyl_len;
			'gal:=vol*7.48052;
		END_ST_ROUTINE

	END_ADD_ON_INSTRUCTION_DEFINITION

